<%= form_for @contact do |c| %>
    <%= c.label :name %>
    <%= c.text_field :name %>

    <%= c.label :email %>
    <%= c.text_field :email %>

    <%= c.label :phone %>
    <%= c.number_field :phone %>

    <%= c.label :message %>
    <%= c.text_area :message %>

    <%= c.submit "Add" %>
<% end %>